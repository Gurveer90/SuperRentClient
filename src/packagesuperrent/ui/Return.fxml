        <Button layoutX="600.0" layoutY="287.0" mnemonicParsing="false" onAction="#ReturnedVehicle" text="Returned Vehicles List" />
      <Label layoutX="13.0" layoutY="28.0" text="Rent Id" />
      <Label layoutX="10.0" layoutY="67.0" text="Odometer" />
      <Label layoutX="11.0" layoutY="207.0" text="Gas Rank" />
    </children>
